<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="1240.0"
            style="-fx-background-color: #cccdcd;" xmlns="http://javafx.com/javafx/8.0.172-ea"
            fx:controller="sample.components.CopySettingComponent">
    <children>
        <AnchorPane layoutX="383.0" layoutY="252.0" prefHeight="298.0" prefWidth="520.0"
                    style="-fx-background-color: White;" AnchorPane.leftAnchor="383.0" AnchorPane.topAnchor="252.0">
            <children>
                <AnchorPane prefHeight="50.0" prefWidth="520.0" style="-fx-background-color: #18749F;">
                    <children>
                        <Label layoutX="152.0" layoutY="7.0" text="Edytuj egzemplarz" textFill="WHITE">
                            <font>
                                <Font name="System Bold" size="25.0"/>
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <Label layoutX="41.0" layoutY="87.0" text="Kod">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <TextField fx:id="code" layoutX="135.0" layoutY="83.0" prefHeight="25.0" prefWidth="333.0"
                           style="-fx-background-color: white; -fx-border-color: #A2B5C6;"/>
                <Label layoutX="40.0" layoutY="126.0" text="Książka">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Button layoutX="384.0" layoutY="251.0" mnemonicParsing="false" onAction="#acceptAdd" prefHeight="31.0"
                        prefWidth="94.0" style="-fx-background-color: #30507F;" text="Zatwierdz" textFill="WHITE">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Button>
                <Button layoutX="35.0" layoutY="251.0" mnemonicParsing="false" onAction="#cancel" prefHeight="31.0"
