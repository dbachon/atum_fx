                <TextField fx:id="bookFilter" layoutX="135.0" layoutY="124.0" prefHeight="25.0" prefWidth="333.0"
                           style="-fx-background-color: white; -fx-border-color: #A2B5C6;"/>
                <ChoiceBox fx:id="availability" layoutX="135.0" layoutY="200.0" prefHeight="25.0" prefWidth="333.0"
                           style="-fx-background-color: white; -fx-border-color: #A2B5C6;"/>
                <Label layoutX="44.0" layoutY="204.0" text="Status">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
